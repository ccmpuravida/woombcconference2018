
@{
    ViewBag.Title = "Galería";
    ViewBag.Description = "Galería de fotos del Congreso Internacional WOOMB.";
}

@section styles {
    <style>
        p {
            margin: 0 0 2em 0;
            text-align: justify;
            text-justify: inter-word;
        }
    </style>
}

<!-- Main -->
<article id="main">
    <header>
        <h2>Galería de Fotos</h2>
        <p style="text-align:center;">Resumen en fotografías de las vivencias de la Conferencia Internacional WOOMB, San José, Costa Rica, 2018.</p>
    </header>
    <section class="wrapper style5">
        <div class="inner">

            <section>
                <iframe width="850" height="478" src="https://myalbum.com/embed/yqeO9LK9Tmhy" frameborder="0" allowfullscreen></iframe>
            </section>

        </div>
    </section>
</article>
@section scripts {
    <script src="~/Content/secondaryjs.js"></script>
}